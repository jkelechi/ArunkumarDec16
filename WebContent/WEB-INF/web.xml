<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Arunkumar</display-name>
	<welcome-file-list>
		<welcome-file>homepage.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>RegisterServlet</display-name>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>com.controller.RegisterServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>SearchServlet</display-name>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>com.controller.SearchServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>ContactServlet</display-name>
		<servlet-name>ContactServlet</servlet-name>
		<servlet-class>com.controller.ContactServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>ProfileServlet</display-name>
		<servlet-name>ProfileServlet</servlet-name>
		<servlet-class>com.controller.ProfileServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>UpdateServlet</display-name>
		<servlet-name>UpdateServlet</servlet-name>
		<servlet-class>com.controller.UpdateServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>ViewTicket</display-name>
		<servlet-name>ViewTicket</servlet-name>
		<servlet-class>com.controller.ViewTicket</servlet-class>
	</servlet>
	<servlet>
		<display-name>ConfirmServlet</display-name>
		<servlet-name>ConfirmServlet</servlet-name>
		<servlet-class>com.controller.ConfirmServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/SearchServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ContactServlet</servlet-name>
		<url-pattern>/ContactServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/ProfileServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateServlet</servlet-name>
		<url-pattern>/UpdateServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ConfirmServlet</servlet-name>
		<url-pattern>/ConfirmServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewTicket</servlet-name>
		<url-pattern>/ViewTicket</url-pattern>
	</servlet-mapping>
</web-app>